        const options = {
            root: null,
            rootMargin: '0px',
            threshold: 0.5 // Trigger the callback when 50% of the div is visible
        };

        const callback = (entries, observer) => {
            entries.forEach(entry => {
                if (entry.isIntersecting) {
                    // The div is now visible, so apply the transition
                    customDiv.style.transform = 'scale(1.1)';
                } else {
                    // The div is not visible, remove the transition
                    customDiv.style.transform = 'scale(1)';
